/*统计*/

.sta-content{
  padding: 20px;
  background-color: #fff;
  min-width: 960px;
  margin-bottom: 30px;
}
.sta-content .count-box{
  margin-bottom: 20px;
}
.sta-content .count{
  color: #333;
  font-size: 12px;
  padding-right: 20px;
  margin-right: 20px;
  border-right: 1px solid #EEEEEE;
  line-height: 24px;
}

.sta-content .count:last-of-type(1){
  border-right: none;
}

/*of*/
.count-input{
  margin-right: 6px;
  border:1px solid #DDDDDD;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 5px;
  position: relative;
  height: 24px;
}
.count-input .search{
  width: 16px;
  height: 16px;
}

.count-input .down{
  width: 12px;
  height: 8px;
  position: absolute;
  top:6px;
  right: 5px;
}
.count-input input{
  width: 130px;
  height:24px;
  line-height: 24px;
  background:#FFFFFF;
  border: none;
  border-radius:1px;
  outline: none;
  padding: 0 5px;
}
/*of*/
.sta-content .label-box{
  margin-bottom: 20px;
  font-size: 12px;
}
.sta-content .label-item{
  margin-right: 30px;
  color: #333;
  cursor: pointer;
  position: relative;

}
.sta-content .check-box-wrap{
  width: 16px;
  height: 16px;
  margin-right: 13px;
  box-sizing: border-box;
  position:relative;
  border-radius: 2px;
  transition: border-color .2s ease-in-out,background-color .2s ease-in-out,box-shadow .2s ease-in-out,-webkit-box-shadow .2s ease-in-out;
}
.sta-content .check-box-wrap.active{
  background-color: #fff;
}
.sta-content .check-box-wrap:after{
  content: "";
  display: table;
  width: 4px;
  height: 8px;
  position: absolute;
  top: 1px;
  left: 4px;
  border: 2px solid #fff;
  border-top: 0;
  border-left: 0;
  -webkit-transform: rotate(45deg) scale(1);
  transform: rotate(45deg) scale(1);
  -webkit-transition: all .2s ease-in-out;
  transition: all .2s ease-in-out;
}
.sta-content .wenhao-icon{
  height: 13px;
  width: 13px;
  margin-left: 3px;
}
.sta-content .checkbox-1{
  border: 1px solid #3591F0;
}
.sta-content .checkbox-1.active{
  background-color: #3591F0;
}
.sta-content .checkbox-2{
  border: 1px solid red;
}
.sta-content .checkbox-2.active{
  background-color: red;
}
.sta-content .checkbox-3{
  border: 1px solid #16D0FF;
}
.sta-content .checkbox-3.active{
  background-color: #16D0FF;
}
.sta-content .checkbox-4{
  border: 1px solid #2A9FF6;
}
.sta-content .checkbox-4.active{
  background-color: #2A9FF6;
}
.sta-content .checkbox-5{
  border: 1px solid #FF0A8F;
}
.sta-content .checkbox-5.active{
  background-color: #FF0A8F;
}
.sta-content .checkbox-6{
  border: 1px solid #890AFF;
}
.sta-content .checkbox-6.active{
  background-color: #890AFF;
}
.sta-content .checkbox-7{
  border: 1px solid #01E6A3;
}
.sta-content .checkbox-7.active{
  background-color: #01E6A3;
}
.sta-content .label-tips{
  position: absolute;
  bottom: 16px;
  left: 30px;
  padding: 7px 0 10px 0;
  z-index: 101;
}
.sta-content .tips-content{
  padding: 28px 20px;
  color: #354052;
  font-size: 12px;
  width: 210px;
  background-color: #fff;
  border: 1px solid #dfe2e6;
  box-shadow: 0 6px 16px 0 rgba(127,143,164,.15);
  border-radius: 4px;
  position: relative;
}
.sta-content .tips-content:after{
  content: '';
  position:absolute;
  height: 10px;
  width: 10px;
  transform: rotate(-45deg);
  border-left:1px solid #dfe2e6;
  border-bottom:1px solid #dfe2e6;
  bottom: -6px;
  left: 20px;
  background-color: #fff;
}
