body{
  background-color: #f1f1f1;
}
button:focus{
  outline:0;
}
.ms-header{
  position: absolute;
  z-index: 99;
  height: 80px;
  width: 100%;
  background-color: #fff;
  min-width: 960px;
}
.ms-header .ms-logo-box{
  float: left;
  width: 260px;
  height: 80px;
  background-color: #3AE3E6;
  color: #fff;
  font-size: 16px;
}
.ms-qianming-icon{
  margin-right: 10px;
  width: 34px;
  height: 34px;
}
.user-info-box{
  padding: 20px;
  float: right;
  font: 18px;
  color: #333;
  cursor: pointer;
  position: relative;
}
.user-info-box:hover .extra-box{
  display: block;
}
.extra-box{
  background-color: #fff;
  height: 100px;
  width: 106px;
  bottom: -100px;
  left: 50%;
  margin-left: -54px;
  display: none;
  position: absolute;
  padding: 10px 0 6px;
  box-sizing: border-box;
}
.extra-item{
  padding: 0 16px;
  color: #333;
  font-size: 12px;
  height: 42px;
  line-height: 42px;
}
.extra-item:hover{
  color: #fff;
  background-color: #3AE3E6;
}
.header-arrow{
  margin: 0 18px 0 5px;
}
.ms-left{
  z-index: 90;
  padding-top: 80px;
  box-sizing: border-box;
  width: 260px;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  overflow: auto;
  background-color: #fff;
}
.ms-left-item{
  width: 100%;
  padding-left: 27px;
  border-left: 4px solid transparent;
  font-size: 16px;
  color: #999;
  box-sizing: border-box;
  height: 80px;
  cursor: pointer;
}
.ms-left-item:hover{
  color: #3AE3E6;
}
.ms-left-item.active{
  color: #3AE3E6;
}
.left-item-icon{
  width: 24px;
  height: 24px;
  margin-right: 13px;
  background-position: center;
  background-repeat: no-repeat;
}

.ms-left-item:hover .left-item-icon1{
  background-image: url(../images/gaishu-1.png);
}
.ms-left-item:hover .left-item-icon2{
  background-image: url(../images/zhanghu-1.png);
}
.ms-left-item:hover .left-item-icon3{
  background-image: url(../images/tongji-1.png);
}
.ms-left-item.active .left-item-icon1{
  background-image: url(../images/gaishu-1.png);
}
.ms-left-item.active .left-item-icon2{
  background-image: url(../images/zhanghu-1.png);
}
.ms-left-item.active .left-item-icon3{
  background-image: url(../images/tongji-1.png);
}
.left-item-icon1{
  background-image: url(../images/gaishu.png);
}
.left-item-icon2{
  background-image: url(../images/zhanghu.png);
}
.left-item-icon3{
  background-image: url(../images/tongji.png);
}
.fabu-btn{
  cursor: pointer;
  margin: 72px auto;
  width: 214px;
  height: 50px;
  border-radius: 8px;
  background-color: #3AE3E6;
  color: #fff;
}

.fabu-icon{
  width: 26px;
  height: 26px;
  margin-right: 10px;
}
.ms-content{
  height: 100%;
  width: 100%;
  padding: 110px 50px 0 290px;
  box-sizing: border-box;
  overflow: auto;
  
}
.layer{
  height: 100%;
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.5)
}
.layer-wrap{
  background-color: #fff;
  border-radius: 10px;
  padding: 15px 20px;
  width: 600px;
  margin: 0 auto;
  position: absolute;
  left:50%;
  top:40%;
  transform: translate(-50%, -50%);
  z-index: 100;
}
.layer-title{
  font-size: 16px;
  color: #666;
}
.layer-remove-icon{
  cursor: pointer
}
.add-app-box{
  padding: 40px 80px;
}
.flow-box{
  width: 480px;
} 
.flow-box .step-name{
  font-size: 14px;
  line-height: 1;
  color: #666;
  margin-top: 25px;
}
.flow-box .step{
  width: 40px;
  height: 40px;
  border-radius: 50%;
  box-sizing: border-box;
  border: 1px solid #999;
  color: #999;
  text-align: center;
  line-height: 38px;
  font-size: 18px;
}
.flow-box .active .step{
  color: #fff;
  border-color: #3AE3E6;
  background-color: #3AE3E6;
}
.flow-box .active .step-name{
  color:  #333;
}
.line{
  margin-bottom: 39px;
  height: 1px;
  width: 140px;
  background-color: #f0f0f0;
}
.upload-btn{
  width: 202px;
  height: 50px;
  line-height: 50px;
  border-radius: 3px;
  background-color: #3AE3E6;
  color: #fff;
  margin: 50px auto;
  text-align: center;
  cursor: pointer;
  position: relative;
}
.upload-btn input{
  width: 202px;
  height: 50px;
  position: absolute;
  top:0;
  left: 0;
  opacity: 0;
}
.tips-box{
  text-align: center;
  font-size: 14px;
  color: #B3B3B3;
  line-height: 24px;
  margin-bottom: 30px;
}
/*** 注册 ***/
.of-reg{
  width: 100%;
  min-width: 1300px;
  min-height: 100vh;
  background: url("../images/logindebg.png") no-repeat;
  background-size: cover;
}
.of-reg .login-main{
  width: 1205px;
  height: 665px;
  background: url("../images/login-bg.png") no-repeat;
  background-size: 100% 100%;
  margin: 0 auto;
  padding: 23px 29px 28px 20px;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.login-main .right-msg{
  width: auto;
  height: 665px;
  display: inline-block;
}
.of-reg .login-box{
  flex: 1;
  padding: 90px 53px 80px 46px;
  text-align: left;
  position: relative;
}
.of-header{
  font-size:36px;
  font-weight:bold;
  color:#3AE3E6;
  margin-bottom: 28px;
}
.login-box .right-light{
  position: absolute;
  top:0;
  right:50px;
  z-index: 3;
}
.of-input{
  width: 385px;
  height: 57px;
  border:1px solid #DCDCDC ;
  border-radius:8px;
  padding: 0 16px;
  margin-bottom: 20px;
}
.of-input img{
  width: 24px;
  height: 24px;
}
.of-input input{
  display: flex;
  margin-left: 16px;
  flex: 1;
  height: 100%;
  outline: none;
  border: none;
  font-size: 18px;
}
input::-webkit-input-placeholder,input:-moz-placeholder,input::-moz-placeholder, input:-ms-input-placeholder{
  color:#DCDCDC
}
input::placeholder{
  color:#DCDCDC
}

.of-pass-btn{
  width: 400px;
  height: 57px;
  line-height: 57px;
  text-align: center;
  font-size: 20px;
  /*background: url("../images/login-btn.png") center no-repeat;*/
  background-size: contain;
  color:#FFFFFF;
  margin: 0 auto 38px;
  background-color: #3ae3e6;
  display: block;
  border: none;
  border-radius: 30px;
}
.of-without{
  font-size: 20px;
  text-align: center;
  color:#999999;
}
.of-reg a{
  color:#2A9FF6;
  font-size: 18px;
  text-decoration: none;
}
.of-agree{
  padding-bottom: 40px;
}
.of-agree a{
  font-size: 16px;
}
.of-agree span{
  width: 18px;
  height: 18px;
  border-radius: 6px 6px;
  /*border: 1px solid #CCCCCC;*/
  background: rgba(204,204,204,0.4);
  display: inline-block;
  position: relative;
  top:5px;
  margin-right: 10px;
  cursor: pointer;
}
.of-agree span.active{
  background: url("../images/login-agree.png");
}

